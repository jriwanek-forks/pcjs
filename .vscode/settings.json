{
  "files.exclude": {
    ".history": true,
    ".sass-cache": true,
    "node_modules": true,
    ".fseventsd": true,
    ".Trashes": true
  },
  "editor.wordWrap": "off",
  "editor.rulers": [127],
  "[javascript]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true
  },
  "[markdown]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "files.trimTrailingWhitespace": false,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true
  },
  "[masm]": {
    "editor.insertSpaces": false,
    "editor.tabSize": 8,
    "files.eol": "\r\n",
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true
  },
  "css.lint.emptyRules": "ignore",
  "css.validate": false,
  "scss.validate": false,
  "stylelint.configOverrides": {
    "ignoreFiles": [
      "./_includes/*.html",
      "./assets/css/*.scss"
    ]
  },
