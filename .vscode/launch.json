  "version": "0.2.0",
  "configurations": [
    {
      "name": "Node: gulp.js",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "console": "internalConsole",
      "outFiles": [
        "${workspaceFolder}/none/**/*.js"
      ]
    },
    {
      "name": "Node: diskdump.js (deprecated)",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/tools/old/diskdump/bin/diskdump",
      "args": [
        "--disk=gamedisks/pcx86/game/other/1984/kings_quest1/archive/disk1.psi",
        "--format=json",
        "--output=disk1.json",
        "--overwrite"
      ],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "console": "integratedTerminal",
      "outFiles": [
        "${workspaceFolder}/none/**/*.js"
      ]
    },
    {
      "name": "Node: diskimage.js",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/tools/modules/diskimage.js",
      "args": [
        "--all",
        "--checkpage",
        "--rebuild"
      ],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "console": "integratedTerminal",
      "outFiles": [
        "${workspaceFolder}/none/**/*.js"
      ]
    },
    {
      "name": "Node: filedump.js",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/tools/old/filedump/bin/filedump",
      "args": [
        "--file=machines/palm/pilot/roms/pilot.rom",
        "--format=bytes",
        "--decimal"
      ],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "console": "integratedTerminal",
      "outFiles": [
        "${workspaceFolder}/none/**/*.js"
      ]
    },
    {
      "name": "Node: pdfparse.js",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/tools/old/textout/lib/pdfparse.js",
      "args": [
        "archive/Whats_In_Print_1984-OCR.pdf",
        "--debug"
